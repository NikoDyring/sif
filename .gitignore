# Ignore Master keys
/sifadmin/config/master.key
/sifweb/config/master.key

# Ignore standard Rails temp and log files
/tmp
/log
*.log
*.pid
*.pid.lock
*.sqlite3
*.sqlite3-journal
*.swp

# Ignore database configs and credentials
/config/database.yml
/config/credentials.yml.enc

# Ignore bundler config
/.bundle

# Ignore Byebug command history file.
.byebug_history

# Ignore node modules and yarn lock file
/node_modules
/yarn-error.log

# Ignore dotenv files for environment variables
.env
.env.*.local

# Ignore coverage generated by tools like SimpleCov
/coverage/

# Ignore assets built by Webpacker or asset pipeline
/public/packs
/public/assets
.byebug_history
/public/packs-test
/node_modules
/yarn-error.log
